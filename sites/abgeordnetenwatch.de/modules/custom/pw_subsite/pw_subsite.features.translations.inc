  );
  $translations['de:default'][20562] = array(
    'lid' => '20562',
    'source' => 'The @drupal installer requires that you create a settings file as part of the installation process. Copy the %default_file file to %file. More details about installing Drupal are available in <a href="@install_txt">INSTALL.txt</a>.',
    'context' => '',
    'location' => '',
    'translation' => 'Das @drupal-Installationsprogramm benötigt für den Installationsvorgang sowie für das Betreiben der Website eine Einstellungsdatei. Kopieren Sie die %default_file Datei nach %file. Weitere Details zur Installation von Drupal sind in der <a href="@install_txt">INSTALL.txt</a> enthalten.',
